10	Manju	35	Hasan	KA

================================================================
Create the table with two partition columns state, district...

create table demographics(sno int, name string, age int) partitioned by (dist string, state string) ROW FORMAT DELIMITED FIELDS TERMINATED BY '\t';

insert into table demographics partition (dist='Nellore', state='AP') select sno, name, age from population where dist='Prakasam' and state='AP';

insert into table demographics partition (dist='Prakasam', state='AP') select sno, name, age from population where dist='Prakasam' and state='AP';
                                                                         select * from demographics;

2	siva	32	Nellore	AP
5	giri	35	Nellore	AP
1	naga	27	Prakasam	AP
4	ravi	30	Prakasam	AP
9	indira	24	Prakasam	AP

select * from demographics where dist='Prakasam' and state = 'AP';

1	naga	27	Prakasam	AP
4	ravi	30	Prakasam	AP
9	indira	24	Prakasam	AP
